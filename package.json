    "netlify-lambda": "^1.4.3",
    "prettier": "^1.16.4",
    "winston": "^3.2.1"
  },
  "license": "MIT",
  "scripts": {
    "build": "yarn clean:build && yarn updateGames && gatsby build",
    "develop": "yarn clean:build && gatsby develop",
    "develop:all": "concurrently \"npm:develop\" \"npm:serve:functions\"",
    "serve": "yarn build && gatsby serve",
    "clean:build": "rm -rf public .cache",
    "clean:all": "rm -rf node_modules && yarn clean:build",
    "format": "prettier --write 'src/**/*.js' --config-precedence file-override",
    "serve:functions": "netlify-lambda serve functions",
    "build:functions": "netlify-lambda build functions",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "updateGames": "node ./scripts/updateAllGames.js"
  }
}
